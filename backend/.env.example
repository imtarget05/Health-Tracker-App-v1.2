# Copy this file to .env and fill values for local development
# Required by src/config/env.js
FIREBASE_API_KEY=your_firebase_web_api_key_here
GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
JWT_SECRET=replace_with_a_strong_secret
AI_SERVICE_URL=http://localhost:8000
AI_CHAT_API_KEY=your_ai_service_key_here

# Firebase Admin service account fields (put values from service account JSON)
# If you paste the private key into an env var, make sure newlines are escaped (\n)
FIREBASE_TYPE=service_account
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_PRIVATE_KEY_ID=your_private_key_id
FIREBASE_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n
FIREBASE_CLIENT_EMAIL=service-account@your-project-id.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_client_id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
FIREBASE_AUTH_PROVIDER_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
FIREBASE_CLIENT_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/... 
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# Local development helpers
USE_FIREBASE_EMULATOR=0
DISABLE_SCHEDULER=1
